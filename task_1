day = int(input())
mounth = int(input())
daym = ["первое", "второе", "третье", "четвёртое", "пятое", "шестое", "седьмое", "девятое", "десятое",
"одиннадцатое", "двеннадцатое", "тринадцатое", "пятнадцатое", "шестнадцатое", "семнадцатое", "восемнадцатое", "девятнадцатое", "двадцатое",
"двадцать", "тридцать"]
mounthm = ["Января", "Февраля", "Марта", "Апреля", "Мая", "Июня", "Июля", "Августа", "Сентября", "Октября", "Ноября", "Декабря"]
print(daym[day-1]+" "+mounthm[mounth-1])
